		<processingElem type="derived">
			<dataSource alias="A1"><![CDATA[SORTBYBASE]]></dataSource>
			<dataGroupName><![CDATA[ADD_DF]]></dataGroupName>
			<ui>
				<position x="522" y="381" />
				<description><![CDATA[]]></description>
			</ui>
			<addColumns>
				<derivedColumn name="APRICE"><![CDATA[({HIGH_P} + {LOW_P}) / 2]]></derivedColumn>
				<derivedColumn name="RPRICE"><![CDATA[IIF( @ROWNUM = 1, (A := {LAST_P}) / {LAST_P},
  IIF( {P_CODE} <> {P_CODE}[-1], (A := {LAST_P}) / {LAST_P}, {LAST_P} / A )
)]]></derivedColumn>
				<derivedColumn name="WEEK_NO"><![CDATA[SUBSTR({DATE}, 1, 4) + 'W' + STR(GETWEEKOFYEAR(STRTODATE({DATE}, 'yyyyMMdd')), 2)]]></derivedColumn>
				<derivedColumn name="DX"><![CDATA[@ROWNUM * 0.01]]></derivedColumn>
				<derivedColumn name="MV5"><![CDATA[IIF( @ROWNUM = 1, MOVINGAVG(@{RPRICE}, MIN(@ROWNUM - (M := @ROWNUM) + 1, 5)),
  IIF( {P_CODE} <> {P_CODE}[-1], MOVINGAVG(@{RPRICE}, MIN(@ROWNUM - (M := @ROWNUM) + 1, 5)),
    MOVINGAVG(@{RPRICE}, MIN(@ROWNUM - M, 5)) )
)]]></derivedColumn>
				<derivedColumn name="DMV5"><![CDATA[IIF( @ROWNUM = 1, 0,
  IIF( {P_CODE} <> {P_CODE}[-1], 0, ({MV5} - {MV5}[-1]) / {MV5}[-1] )
)]]></derivedColumn>
				<derivedColumn name="DMV5MV5"><![CDATA[IIF( @ROWNUM = 1, MOVINGAVG(@{DMV5}, MIN(@ROWNUM - (M := @ROWNUM) + 1, 5)),
  IIF( {P_CODE} <> {P_CODE}[-1], MOVINGAVG(@{DMV5}, MIN(@ROWNUM - (M := @ROWNUM) + 1, 5)),
    MOVINGAVG(@{DMV5}, MIN(@ROWNUM - M, 5)) )
)]]></derivedColumn>
				<derivedColumn name="SLP_MV5"><![CDATA[IIF(@ROWNUM < 5, 0, LINEARSLOPE(@{DX}, @{MV5}, 5))]]></derivedColumn>
				<derivedColumn name="SLP_5D"><![CDATA[IIF(@ROWNUM < 5, 0, LINEARSLOPE(@{DX}, @{RPRICE}, 5))]]></derivedColumn>
			</addColumns>
		</processingElem>

