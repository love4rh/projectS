		<processingElem type="derived">
			<dataSource alias="A1"><![CDATA[SORTBYBASE]]></dataSource>
			<dataGroupName><![CDATA[ADD_DF]]></dataGroupName>
			<ui>
				<position x="521" y="345" />
				<description><![CDATA[]]></description>
			</ui>
			<addColumns>
				<derivedColumn name="RPRICE_ORG"><![CDATA[IIF( @ROWNUM = 1, (A := {LAST_P}) / {LAST_P},
  IIF( {P_CODE} <> {P_CODE}[-1], (A := {LAST_P}) / {LAST_P}, {LAST_P} / A )
)]]></derivedColumn>
				<derivedColumn name="RPRICE"><![CDATA[{RPRICE_ORG}]]></derivedColumn>
				<derivedColumn name="WEEK_NO"><![CDATA[SUBSTR({DATE}, 1, 4) + 'W' + STR(GETWEEKOFYEAR(STRTODATE({DATE}, 'yyyyMMdd')), 2)]]></derivedColumn>
				<derivedColumn name="DX"><![CDATA[@ROWNUM * 0.1]]></derivedColumn>
				<derivedColumn name="MV5"><![CDATA[IIF( @ROWNUM = 1, MOVINGAVG(@{RPRICE}, (M := 1)),
  IIF( {P_CODE} <> {P_CODE}[-1],
    MOVINGAVG(@{RPRICE}, (M := @ROWNUM) / @ROWNUM),
    MOVINGAVG(@{RPRICE}, MIN(3, @ROWNUM - M))
  )
)]]></derivedColumn>
				<derivedColumn name="SLP_MV5"><![CDATA[IIF(@ROWNUM < 5, 0, LINEARSLOPE(@{DX}, @{RPRICE}, 5))]]></derivedColumn>
				<derivedColumn name="CUM_SLP"><![CDATA[IIF( @ROWNUM = 1, {SLP_MV5},
  IIF( {P_CODE} <> {P_CODE}[-1], {SLP_MV5}, GETVALUE(@THIS, -1) + {SLP_MV5} )
)]]></derivedColumn>
			</addColumns>
		</processingElem>

