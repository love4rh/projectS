			<dataGroupName><![CDATA[VALID]]></dataGroupName>
			<ui>
				<position x="67" y="232" />
				<description><![CDATA[]]></description>
			</ui>
			<condition><![CDATA[IIF( ISNULL({DT}), 0, IIF( LENGTH({DT}) >= 8, 1, 0 ) )]]></condition>
