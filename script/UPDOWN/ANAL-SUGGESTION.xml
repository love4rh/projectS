			<ui>
				<position x="418" y="132" />
				<description><![CDATA[]]></description>
			</ui>
			<addColumns>
				<derivedColumn name="DX"><![CDATA[@ROWNUM * 0.1]]></derivedColumn>
				<derivedColumn name="MV/20"><![CDATA[ROUND(MOVINGAVG(@{LAST_P}, 20), 0)]]></derivedColumn>
				<derivedColumn name="MV/60"><![CDATA[ROUND(MOVINGAVG(@{LAST_P}, 60), 0)]]></derivedColumn>
				<derivedColumn name="MV/120"><![CDATA[ROUND(MOVINGAVG(@{LAST_P}, 120), 0)]]></derivedColumn>
				<derivedColumn name="RMV/20"><![CDATA[{MV/20} / $(RATIO_FACTOR)]]></derivedColumn>
				<derivedColumn name="RMV/60"><![CDATA[{MV/60} / $(RATIO_FACTOR)]]></derivedColumn>
				<derivedColumn name="SLOPE/MV20"><![CDATA[IIF( @ROWNUM < 5, 0, LINEARSLOPE(@{DX}, @{RMV/20}, 5) )]]></derivedColumn>
				<derivedColumn name="SLOPE/MV60"><![CDATA[IIF( @ROWNUM < 5, 0, LINEARSLOPE(@{DX}, @{RMV/60}, 5) )]]></derivedColumn>
				<derivedColumn name="RATIO/MV120"><![CDATA[{LAST_P} / {MV/120}]]></derivedColumn>
				<derivedColumn name="SUGGEST/BUY"><![CDATA[IIF( @ROWNUM < 20, 0,
  IIF( {RATIO/MV120} < 91.05
      AND {SLOPE/MV20} >= 0.02
      AND {MV/20} / {MV/20}[-1] >= 1.002
    , IIF( MOVINGSUM(@THIS, 40) = 0, 1, 0)
    , 0 )
)]]></derivedColumn>
				<derivedColumn name="ACCEL/MV20"><![CDATA[IIF( @ROWNUM < 5, 0, LINEARSLOPE(@{DX}, @{SLOPE/MV20}, 5) )]]></derivedColumn>
			</addColumns>
		</processingElem>

