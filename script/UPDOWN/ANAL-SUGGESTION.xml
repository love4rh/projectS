				<description><![CDATA[]]></description>
			</ui>
			<addColumns>
				<derivedColumn name="GROUP/INDEX"><![CDATA[IIF( @ROWNUM = 1, G := 0,
  IIF( {SUGGEST/BUY}[-1] = 1, G := G + 1, G )
)]]></derivedColumn>
				<derivedColumn name="TMP/BP"><![CDATA[IIF( @ROWNUM = 1, 0,
  IIF( {SUGGEST/BUY}[-1] = 1, {LAST_P}[-1], GETVALUE(@THIS, -1) )
)]]></derivedColumn>
				<derivedColumn name="TMP/RATIO"><![CDATA[IIF( {TMP/BP} = 0, 0, {LAST_P} / {TMP/BP} )]]></derivedColumn>
				<derivedColumn name="TMP/INDEX"><![CDATA[IIF( @ROWNUM = 1, 0,
  IIF( {SUGGEST/BUY}[-1] = 1, @ROWNUM - 1, GETVALUE(@THIS, -1) )
)]]></derivedColumn>
				<derivedColumn name="ELEM/INDEX"><![CDATA[@ROWNUM - {TMP/INDEX}]]></derivedColumn>
				<derivedColumn name="TMP/SELL01"><![CDATA[IIF( @ROWNUM = 1, 0,
   CASE( GETVALUE(@THIS, -1)
      , {ELEM/INDEX}[-1] >= {ELEM/INDEX}, 0
      , {SLOPE/MV20} < 0 AND {TMP/RATIO} <= 0.99, 1
      , {TMP/RATIO} >= 1.16, 2
      , {ELEM/INDEX} > 65, 1
   )
)]]></derivedColumn>
				<derivedColumn name="RETAIN/FLAG"><![CDATA[IIF( {GROUP/INDEX} > 0 AND {TMP/SELL01} = 0 OR {TMP/SELL01}[-1] = 0, 1 , 0 )]]></derivedColumn>
				<derivedColumn name="SUGGEST"><![CDATA[IIF( @ROWNUM = @DATASIZE, 0,
   CASE( 0
       , {SUGGEST/BUY} > 0, 1
       , {RETAIN/FLAG} = 1 AND {RETAIN/FLAG}[1] = 0, 2
   )
)]]></derivedColumn>
			</addColumns>
		</processingElem>

