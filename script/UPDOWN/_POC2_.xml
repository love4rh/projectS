			<addColumns>
				<derivedColumn name="DX"><![CDATA[@ROWNUM * 0.1]]></derivedColumn>
				<derivedColumn name="AMT(K)"><![CDATA[ROUND({AMOUNT} / 1000, 0)]]></derivedColumn>
				<derivedColumn name="S_VALUE"><![CDATA[({LAST_P} - GETSTATISTICS(@{LAST_P}, "min")) / GETSTATISTICS(@{LAST_P}, "range")]]></derivedColumn>
				<derivedColumn name="SV/10"><![CDATA[MOVINGAVG(@{S_VALUE}, 10)]]></derivedColumn>
				<derivedColumn name="SV/40"><![CDATA[MOVINGAVG(@{S_VALUE}, 40)]]></derivedColumn>
				<derivedColumn name="SV/80"><![CDATA[MOVINGAVG(@{S_VALUE}, 80)]]></derivedColumn>
				<derivedColumn name="MV/10"><![CDATA[ROUND( MOVINGAVG(@{LAST_P}, 10), 0 )]]></derivedColumn>
				<derivedColumn name="MV/40"><![CDATA[ROUND( MOVINGAVG(@{LAST_P}, 40), 0 )]]></derivedColumn>
				<derivedColumn name="MV/80"><![CDATA[ROUND( MOVINGAVG(@{LAST_P}, 80), 0 )]]></derivedColumn>
				<derivedColumn name="STD/10"><![CDATA[IIF( @ROWNUM < 10, 0, SQRT(MOVINGVAR(@{SV/10} , 3)) * 1000 )]]></derivedColumn>
				<derivedColumn name="STD/40"><![CDATA[IIF( @ROWNUM < 40, 0, SQRT(MOVINGVAR(@{SV/40} , 5)) * 1000 )]]></derivedColumn>
				<derivedColumn name="STD/80"><![CDATA[IIF( @ROWNUM < 80, 0, SQRT(MOVINGVAR(@{SV/80} , 5)) * 1000 )]]></derivedColumn>
				<derivedColumn name="SLOPE/T20"><![CDATA[IIF( @ROWNUM < 20, 0, LINEARSLOPE(@{DX}, @{S_VALUE}, 20) * 100 )]]></derivedColumn>
				<derivedColumn name="SLOPE/20"><![CDATA[IIF( 0 < {SLOPE/T20} AND {SLOPE/T20} < $(ZERO_LIMIT), 0, {SLOPE/T20} )]]></derivedColumn>
				<derivedColumn name="SLOPE/T40"><![CDATA[IIF( @ROWNUM < 40, 0, LINEARSLOPE(@{DX}, @{S_VALUE}, 40) * 100 )]]></derivedColumn>
				<derivedColumn name="SLOPE/40"><![CDATA[{SLOPE/T40}]]></derivedColumn>
				<derivedColumn name="SLOPE/80"><![CDATA[IIF( @ROWNUM < 80, 0, LINEARSLOPE(@{DX}, @{S_VALUE}, 80) * 100 )]]></derivedColumn>
				<derivedColumn name="RATIO/80"><![CDATA[{LAST_P} / {MV/80}]]></derivedColumn>
				<derivedColumn name="RATIO/MIN"><![CDATA[{MV/10} / MOVINGMIN(@{MV/10}, 200)]]></derivedColumn>
				<derivedColumn name="RATIO/MAX"><![CDATA[{MV/10} / MOVINGMAX(@{MV/10}, 260)]]></derivedColumn>
				<derivedColumn name="AVG/40"><![CDATA[IIF( @ROWNUM < 51, 1, MOVINGAVG(@{SLOPE/40}, 5) )]]></derivedColumn>
				<derivedColumn name="AVG/80"><![CDATA[IIF( @ROWNUM < 91, 1, MOVINGAVG(@{SLOPE/80}, 5) )]]></derivedColumn>
				<derivedColumn name="HL/40"><![CDATA[IIF( @ROWNUM < 51, 1, {SLOPE/40} / IIF( {AVG/40}[-1] = 0, 1, {AVG/40}[-1] ) )]]></derivedColumn>
				<derivedColumn name="HL/80"><![CDATA[IIF( @ROWNUM < 92, 1, {SLOPE/80} / IIF( {AVG/80}[-1] = 0, 1, {AVG/80}[-1] ) )]]></derivedColumn>
				<derivedColumn name="PT/20"><![CDATA[IIF( @ROWNUM < 91 OR {HL/40} = 0, 0,
   CASE( 0
      , {SLOPE/20}[-1] < {SLOPE/40}[-1] AND {SLOPE/20} >= {SLOPE/40}
        AND {SLOPE/40} < -1.0 AND {AVG/40} - 0.5 <= {SLOPE/40}, 3
      , {SLOPE/20}[-1] <= 0 AND {SLOPE/20} > 0
        AND {SLOPE/40} < 0 AND {HL/40} > {AVG/40}[-1]
        AND {SLOPE/80} < -0.5, 4
   )
)]]></derivedColumn>
				<derivedColumn name="PT2/20"><![CDATA[IIF( @ROWNUM < 31, 0,
CASE( 0
    , ({PT/20}[-2] = 3 OR {PT/20}[-1] = 3 OR {PT/20} = 3)
      AND {SLOPE/20} >= {SLOPE/40} + 0.5, 3
    , {PT/20} = 4 AND {SLOPE/20} > 0.9, 1
    , {PT/20}[-1] = 4 AND {SLOPE/20}[-1] < {SLOPE/20}, 1
))]]></derivedColumn>
			</addColumns>
