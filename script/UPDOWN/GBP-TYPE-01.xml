       , GETVALUE(@THIS, -1) = 1
         AND {PT/20} = 5 AND {TMP/RATIO} > 1.05, 0
       , @ROWNUM - {TMP/BP} > 10
         AND MOVINGSUM(@{UP/80}, @ROWNUM - {TMP/BP} - 1) < (@ROWNUM - {TMP/BP}) / 2, 0
       , @ROWNUM - {TMP/BP} > 15 AND {PT/20} = 7, 0
   )
)]]></derivedColumn>
