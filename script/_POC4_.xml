		<processingElem type="orderAndFilter">
			<dataSource alias="A1"><![CDATA[GUESS_BUY]]></dataSource>
			<dataGroupName><![CDATA[DISPLAY_COLUMNS]]></dataGroupName>
			<ui>
				<position x="493" y="207" />
				<description><![CDATA[]]></description>
			</ui>
			<orderAndFilter>
				<column>DATE</column>
				<column>SUGGEST</column>
