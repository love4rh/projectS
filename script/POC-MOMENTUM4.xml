		<processingElem type="derived">
			<dataSource alias="A1"><![CDATA[ADD_DF]]></dataSource>
			<dataGroupName><![CDATA[ADD_EXDF]]></dataGroupName>
			<ui>
				<position x="650" y="346" />
				<description><![CDATA[]]></description>
			</ui>
			<addColumns>
				<derivedColumn name="SHT_VAR"><![CDATA[SQRT(MOVINGVAR(@{SLOPE/MV}, 5))]]></derivedColumn>
				<derivedColumn name="SHTVAR_MV"><![CDATA[MOVINGAVG(@{SHT_VAR}, 5)]]></derivedColumn>
				<derivedColumn name="MIN_IN_YEAR"><![CDATA[MOVINGMIN(@{MV/LP}, 250)]]></derivedColumn>
				<derivedColumn name="MIN_RATIO"><![CDATA[{MV/LP} / {MIN_IN_YEAR}]]></derivedColumn>
				<derivedColumn name="BPOS_FLAG"><![CDATA[IIF( @ROWNUM < 100, 0,
  IIF( (MOVINGSUM(@{CLINE/SMOOTH}, 6) - {CLINE/SMOOTH}) / 5 + 0.001 < {CLINE/SMOOTH}
       AND {SHTVAR_MV}[-1] <= 0.05
       AND MOVINGSUM(@THIS, 30) = 0
       AND {MIN_RATIO} <= 1.2
       , 1, 0 )
)]]></derivedColumn>
				<derivedColumn name="CONT_P_SLP"><![CDATA[IIF( @ROWNUM < 2, 0,
  IIF( {SLOPE/SMOOTH} >= 0, GETVALUE(@THIS, -1) + {SLOPE/SMOOTH}, 0 )
)]]></derivedColumn>
				<derivedColumn name="CONT_M_SLP"><![CDATA[IIF( @ROWNUM < 2, 0,
  IIF( {SLOPE/SMOOTH} <= 0, GETVALUE(@THIS, -1) + {SLOPE/SMOOTH}, 0 )
)]]></derivedColumn>
				<derivedColumn name="CONT_SLP"><![CDATA[{CONT_P_SLP} + {CONT_M_SLP}]]></derivedColumn>
			</addColumns>
		</processingElem>

