		<processingElem type="derived">
			<dataSource alias="A1"><![CDATA[SORTBYBASE]]></dataSource>
			<dataGroupName><![CDATA[ADD_DF]]></dataGroupName>
			<ui>
				<position x="615" y="201" />
				<description><![CDATA[]]></description>
			</ui>
			<addColumns>
				<derivedColumn name="RPRICE"><![CDATA[IIF( @ROWNUM = 1, (A := {종가}) / {종가},
  IIF( {P_CODE} <> {P_CODE}[-1], (A := {종가}) / {종가}, {종가} / A )
)]]></derivedColumn>
				<derivedColumn name="WEEK_NO"><![CDATA[SUBSTR({날짜}, 1, 4) + 'W' + STR(GETWEEKOFYEAR(STRTODATE({날짜}, 'yyyyMMdd')), 2)]]></derivedColumn>
				<derivedColumn name="DX"><![CDATA[@ROWNUM * 0.01]]></derivedColumn>
				<derivedColumn name="SLP_5D"><![CDATA[IIF(@ROWNUM < 5, 0, LINEARSLOPE(@{DX}, @{RPRICE}, 5))]]></derivedColumn>
				<derivedColumn name="LOW_RATE"><![CDATA[{RPRICE} / MOVINGMIN(@{RPRICE}, 120) * 100 - 100]]></derivedColumn>
				<derivedColumn name="HIGH_RATE"><![CDATA[{RPRICE} / MOVINGMAX(@{RPRICE}, 120) * 100]]></derivedColumn>
				<derivedColumn name="REV_HRATE"><![CDATA[100 - {HIGH_RATE}]]></derivedColumn>
			</addColumns>
		</processingElem>
	</dataProcessing>
