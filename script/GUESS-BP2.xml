		<processingElem type="derived">
			<dataSource alias="A1"><![CDATA[SORTBYBASE]]></dataSource>
			<dataGroupName><![CDATA[ADD_FEATURES]]></dataGroupName>
			<ui>
				<position x="291" y="50" />
				<description><![CDATA[]]></description>
			</ui>
			<addColumns>
				<derivedColumn name="DX"><![CDATA[@ROWNUM * 0.1]]></derivedColumn>
				<derivedColumn name="AMT(K)"><![CDATA[ROUND({AMOUNT} / 1000, 0)]]></derivedColumn>
				<derivedColumn name="S_VALUE"><![CDATA[({LAST_P} - GETSTATISTICS(@{LAST_P}, "min")) / GETSTATISTICS(@{LAST_P}, "range")]]></derivedColumn>
				<derivedColumn name="SV/20"><![CDATA[MOVINGAVG(@{S_VALUE}, 20)]]></derivedColumn>
				<derivedColumn name="SV/60"><![CDATA[MOVINGAVG(@{S_VALUE}, 60)]]></derivedColumn>
				<derivedColumn name="SV/120"><![CDATA[MOVINGAVG(@{S_VALUE}, 120)]]></derivedColumn>
				<derivedColumn name="MV/20"><![CDATA[ROUND( MOVINGAVG(@{LAST_P}, 20), 0 )]]></derivedColumn>
				<derivedColumn name="MV/60"><![CDATA[ROUND( MOVINGAVG(@{LAST_P}, 60), 0 )]]></derivedColumn>
				<derivedColumn name="MV/120"><![CDATA[ROUND( MOVINGAVG(@{LAST_P}, 120), 0 )]]></derivedColumn>
				<derivedColumn name="AVG/20"><![CDATA[IIF( @ROWNUM < 12, 1, (MOVINGSUM(@{MV/20}, 11) - {MV/20}) / 10 )]]></derivedColumn>
				<derivedColumn name="AVG/60"><![CDATA[IIF( @ROWNUM < 12, {MV/60}, (MOVINGSUM(@{MV/60}, 11) - {MV/60}) / 10 )]]></derivedColumn>
				<derivedColumn name="AVG/120"><![CDATA[IIF( @ROWNUM < 140, {MV/120}, (MOVINGSUM(@{MV/120}, 21) - {MV/120}) / 20 )]]></derivedColumn>
				<derivedColumn name="STD/20"><![CDATA[IIF( @ROWNUM < 30, 0, SQRT(MOVINGVAR(@{SV/20} , 5)) * 1000 )]]></derivedColumn>
				<derivedColumn name="STD/60"><![CDATA[IIF( @ROWNUM < 70, 0, SQRT(MOVINGVAR(@{SV/60} , 5)) * 1000 )]]></derivedColumn>
				<derivedColumn name="STD/120"><![CDATA[IIF( @ROWNUM < 130, 0, SQRT(MOVINGVAR(@{SV/120} , 5)) * 1000 )]]></derivedColumn>
				<derivedColumn name="SLOPE/20"><![CDATA[IIF( @ROWNUM < 20, 0, LINEARSLOPE(@{DX}, @{SV/20}, 4) )]]></derivedColumn>
				<derivedColumn name="SLOPE/60"><![CDATA[IIF( @ROWNUM < 60, 0, LINEARSLOPE(@{DX}, @{SV/60}, 4) )]]></derivedColumn>
				<derivedColumn name="RATIO/120"><![CDATA[{LAST_P} / {MV/120}]]></derivedColumn>
				<derivedColumn name="RATIO/MIN"><![CDATA[{MV/20} / MOVINGMIN(@{MV/20}, 200)]]></derivedColumn>
				<derivedColumn name="RATIO/MAX"><![CDATA[{MV/20} / MOVINGMAX(@{MV/20}, 260)]]></derivedColumn>
				<derivedColumn name="HL/20"><![CDATA[IIF( @ROWNUM < 21, 1, {MV/20} / {AVG/20} )]]></derivedColumn>
				<derivedColumn name="HL/A2"><![CDATA[IIF( @ROWNUM = 1, 0, ({HL/20}[-1] + {HL/20}) / 2 )]]></derivedColumn>
				<derivedColumn name="HL/60"><![CDATA[IIF( @ROWNUM < 70, 1, {MV/60} / {AVG/60} )]]></derivedColumn>
				<derivedColumn name="HL/120"><![CDATA[IIF( @ROWNUM < 140, 1, {MV/120} / {AVG/120} )]]></derivedColumn>
				<derivedColumn name="UP/DOWN"><![CDATA[IIF( @ROWNUM < 30, 0.5,
   IIF( {STD/20} < $(FLATLIMIT), 0.5,
      IIF( {SLOPE/20} > 0, 1, 0 )
   )
)]]></derivedColumn>
				<derivedColumn name="AVG/S20"><![CDATA[IIF( @ROWNUM < 10, 0, (MOVINGSUM(@{STD/20}, 6) - {STD/20}) / 5 )]]></derivedColumn>
				<derivedColumn name="PT/20"><![CDATA[IIF( @ROWNUM < 130, 0,
   IIF( MOVINGSUM(@THIS, 25) = 0 AND {STD/20} < 1.125,
      CASE( 0
         , {HL/A2} <= 0.991 AND {MV/20} < {MV/120} AND {HL/20} <= 0.985, 4
         , MOVINGMORECOUNT(@{HL/A2}, 4, 1.00899999) > 0 AND {MV/20} > {MV/120}, 3
      ),
      CASE( 0
         , MOVINGMORECOUNT(@THIS, 45, 4) = 0 AND {STD/20} > 8 AND {HL/20} > 1
           AND {MV/20} > {MV/60} AND {MV/20} > {MV/120}, 5
      )
   )
)]]></derivedColumn>
				<derivedColumn name="PT2/20"><![CDATA[IIF( @ROWNUM < 130, 0,
   CASE( 0
      , {MV/20}[-1] >= {MV/60}[-1] AND {MV/20} <= {MV/60} AND {MV/20}[-1] > {MV/20}, 6
      , {MV/20}[-1] <= {MV/60}[-1] AND {MV/20} >= {MV/60} AND {MV/20}[-1] < {MV/20}, 7
   )
)]]></derivedColumn>
			</addColumns>
		</processingElem>

